<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.PrincipalController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="500.0">
         <left>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#abreTelaExemplo" text="Exemplo">
                     <VBox.margin>
                        <Insets left="8.0" top="8.0" />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#abreTelaSimulado1" text="Simulado 1">
                     <VBox.margin>
                        <Insets left="8.0" top="8.0" />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#abreTelaSimulado2" text="Simulado 2">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </left>
         <top>
            <Label text="Multi thread" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets top="10.0" />
               </BorderPane.margin>
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
         </top>
         <center>
            <AnchorPane BorderPane.alignment="CENTER">
               <children>
                  <TabPane fx:id="tabPane" />
               </children>
            </AnchorPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
